{
  "ENVIRONMENT": "test",
  "BANNER_TITLE_HREF": "https://cps-tst.outsystemsenterprise.com/Casework_Blocks/home",
  "AD_TENANT_AUTHORITY": "https://login.microsoftonline.com/00dd0d1d-d7e6-4338-ac51-565339c7088c",
  "AD_CLIENT_ID": "8d6133af-9593-47c6-94d0-5c65e9e310f1",
  "AD_GATEWAY_SCOPE": "https://CPSGOVUK.onmicrosoft.com/fa-polaris-qa-gateway/user_impersonation",
  "APP_INSIGHTS_KEY": "e572c03c-8d38-4771-b193-962f13da1b1a",
  "SURVEY_LINK": "https://forms.office.com/e/8KM9A7aS0e",
  "SHOW_MENU": true,
  "OS_HANDOVER_URL": "https://cps-tst.outsystemsenterprise.com/Casework_Patterns/auth-handover-test.html",
  "COOKIE_HANDOVER_URL": "https://polaris-qa-notprod.cps.gov.uk/polaris",
  "TOKEN_HANDOVER_URL": "https://polaris-qa-notprod.cps.gov.uk/auth-refresh-cms-modern-token",
  "FEATURE_FLAG_MENU_USERS": {
    "generallyAvailable": true
  },
  "FEATURE_FLAG_ENABLE_INTRUSIVE_AD_LOGIN": false,
  "CONTEXTS": {
    "outsystems": {
      "msalRedirectUrl": "https://cps-tst.outsystemsenterprise.com/Casework_Patterns/auth-handover-test.html",
      "headerCustomCssClasses": "header",
      "headerCustomCssStyles": {
        "background-color": "transparent",
        "box-shadow": "none"
      },
      "contexts": {
        "casework_block": {
          "cmsAuthFromStorageKey": "$OS_Users$Casework_Blocks$ClientVars$JSONString",
          "contexts": {
            "home": {
              "paths": [
                "https://cps-tst.outsystemsenterprise.com/casework_blocks/home"
              ]
            },
            "home-unknown": {
              "paths": [
                "https://cps-tst.outsystemsenterprise.com/casework_blocks"
              ]
            }
          }
        },
        "CaseReview": {
          "cmsAuthFromStorageKey": "$OS_Users$CaseReview$ClientVars$CmsAuthValues",
          "contexts": {
            "review": {
              "paths": [
                "https://cps-tst.outsystemsenterprise.com/CaseReview.*?[&?](?:CMS)?CaseId=(?<caseId>\\d+)"
              ],
              "domTagDefinitions": [
                {
                  "cssSelector": "div#URN_Container",
                  "regex": "<span[^>]*>(?<urn>.*?)</span>"
                }
              ]
            },
            "review-unknown": {
              "paths": ["https://cps-tst.outsystemsenterprise.com/CaseReview"]
            }
          }
        },
        "WorkManagement": {
          "cmsAuthFromStorageKey": "$OS_Users$WorkManagementApp$ClientVars$JSONString",
          "contexts": {
            "details": {
              "paths": [
                "https://cps-tst.outsystemsenterprise.com/WorkManagementApp.*?(?=.*[&?]CaseId=(?<caseId>\\d+))(?=.*[&?](?:Case)?URN=(?<urn>[^&]+))"
              ]
            },
            "cases": {
              "paths": [
                "https://cps-tst.outsystemsenterprise.com/WorkManagementApp/Cases",
                "https://cps-tst.outsystemsenterprise.com/WorkManagementApp/SearchCase"
              ]
            },
            "tasks": {
              "paths": [
                "https://cps-tst.outsystemsenterprise.com/WorkManagementApp/TaskList"
              ]
            },
            "work-management-unknown": {
              "paths": [
                "https://cps-tst.outsystemsenterprise.com/WorkManagementApp"
              ]
            }
          }
        }
      }
    },
    "polaris": {
      "msalRedirectUrl": "https://polaris-qa-notprod.cps.gov.uk/polaris-ui/build-version.txt",
      "contexts": {
        "materials": {
          "paths": [
            "https://polaris-qa-notprod.cps.gov.uk/polaris-ui/case-details/(?<urn>[^/]+)/(?<caseId>\\d+)"
          ]
        },
        "polaris-unknown": {
          "paths": ["https://polaris"]
        }
      }
    },
    "housekeeping": {
      "paths": ["https://housekeeping"],
      "msalRedirectUrl": "https://housekeeping-staging.int.cps.gov.uk/global-components-msal-redirect.html",
      "domTagDefinitions": [
        {
          "cssSelector": "a[href*='/polaris-ui/case-details/']",
          "regex": "/polaris-ui/case-details/.*/(?<caseId>\\d+)\""
        },
        {
          "cssSelector": "div.case-info-details p",
          "regex": ">(?<urn>[^<]+)"
        }
      ]
    },
    "development": {
      "paths": ["(?<scheme>https?)://localhost"],
      "msalRedirectUrl": "{scheme}://localhost"
    }
  },
  "LINKS": [
    {
      "label": "Home",
      "level": 0,
      "href": "https://cps-tst.outsystemsenterprise.com/Casework_Blocks/home",
      "visibleContexts": "home tasks cases case unknown-wma",
      "activeContexts": "home"
    },
    {
      "label": "Tasks",
      "level": 0,
      "href": "https://cps-tst.outsystemsenterprise.com/WorkManagementApp/TaskList",
      "visibleContexts": "home tasks cases case unknown-wma",
      "activeContexts": "tasks"
    },
    {
      "label": "Cases",
      "level": 0,
      "href": "https://cps-tst.outsystemsenterprise.com/WorkManagementApp/Cases?IsCasesNavigation=true",
      "visibleContexts": "home tasks cases case unknown-wma",
      "activeContexts": "cases case"
    },
    {
      "label": "Details",
      "level": 1,
      "href": "https://cps-tst.outsystemsenterprise.com/WorkManagementApp/CaseOverview?CaseId={caseId}&URN={urn}",
      "visibleContexts": "case",
      "activeContexts": "details"
    },
    {
      "label": "Materials",
      "level": 1,
      "href": "https://polaris-qa-notprod.cps.gov.uk/polaris?q=%7B%22caseId%22%3A{caseId}%7D",
      "visibleContexts": "case",
      "activeContexts": "materials"
    },
    {
      "label": "Review",
      "level": 1,
      "href": "https://cps-tst.outsystemsenterprise.com/CaseReview/LandingPage?CMSCaseId={caseId}",
      "visibleContexts": "case",
      "activeContexts": "review"
    }
  ]
}
