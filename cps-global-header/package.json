{
  "name": "cps-global-header",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "test.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/cps-global-header/cps-global-header.esm.js",
  "exports": {
    ".": {
      "import": "./dist/cps-global-header/cps-global-header.esm.js",
      "require": "./dist/cps-global-header/cps-global-header.cjs.js"
    },
    "./cps-global-header": {
      "import": "./dist/components/cps-global-header.js",
      "types": "./dist/components/cps-global-header.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CPS-Innovation/global-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "dotenvx run -f .env.local -- stencil build && rollup -c",
    "dev": "dotenvx run -f .env.local -- stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watchAll",
    "deploy:unstable": "dotenvx run -f .env.unstable -- bash ./deploy.sh",
    "deploy:dev": "dotenvx run -f .env.dev -- bash ./deploy.sh",
    "deploy:test": "dotenvx run -f .env.test -- bash ./deploy.sh",
    "deploy:prod": "dotenvx run -f .env.prod -- bash ./deploy.sh",
    "deploy:outsystems": "dotenvx run -f .env.outsystems -- bash ./deploy.sh",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@stencil/core": "^4.7.0",
    "@stencil/sass": "^3.0.12",
    "@types/jest": "^29.5.6",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "navigation-api-types": "^0.6.1",
    "puppeteer": "^24.0.0",
    "rollup": "^4.40.0"
  },
  "license": "MIT",
  "dependencies": {
    "@azure/msal-browser": "^4.0.0",
    "@dotenvx/dotenvx": "^1.41.0",
    "@microsoft/applicationinsights-web": "^3.3.6",
    "fetch-jsonp": "^1.3.0",
    "govuk-frontend": "^5.7.1",
    "http-server": "^14.1.1",
    "uuid": "^11.0.4"
  }
}
