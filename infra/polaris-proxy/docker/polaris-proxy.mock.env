# Mock environment variables for polaris-proxy testing
# These are used by docker-compose to provide values for nginx template substitution

# Core nginx settings
WEBSITE_DNS_SERVER=127.0.0.11
WEBSITE_HOSTNAME=localhost
WEBSITE_SCHEME=http

# Rate limiting
CMS_RATE_LIMIT=100r/s
CMS_RATE_LIMIT_QUEUE=50

# Task list
WM_TASK_LIST_HOST_NAME=http://mock-upstream:3000

# Protocol for internal endpoints
ENDPOINT_HTTP_PROTOCOL=http

# Auth handover
AUTH_HANDOVER_ENDPOINT_DOMAIN_NAME=mock-upstream:3000
AUTH_HANDOVER_WHITELIST=/auth-refresh-inbound,http://localhost

# App paths and endpoints
APP_SUBFOLDER_PATH=polaris-ui
APP_ENDPOINT_DOMAIN_NAME=mock-upstream:3000
API_ENDPOINT_DOMAIN_NAME=mock-upstream:3000
SAS_URL_DOMAIN_NAME=mock-upstream:3000

# DDEI
DDEI_ENDPOINT_DOMAIN_NAME=mock-upstream:3000
DDEI_ENDPOINT_FUNCTION_APP_KEY=mock-function-key

# Function app key
ENDPOINT_FUNCTION_APP_KEY=mock-function-key

# DEFAULT CMS environment (cin3 / production)
DEFAULT_UPSTREAM_CMS_IP_CORSHAM=mock-upstream:3000
DEFAULT_UPSTREAM_CMS_MODERN_IP_CORSHAM=mock-upstream:3000
DEFAULT_UPSTREAM_CMS_IP_FARNBOROUGH=mock-upstream:3000
DEFAULT_UPSTREAM_CMS_MODERN_IP_FARNBOROUGH=mock-upstream:3000
DEFAULT_UPSTREAM_CMS_DOMAIN_NAME=cms-default.cps.gov.uk
DEFAULT_UPSTREAM_CMS_SERVICES_DOMAIN_NAME=services-default.cps.gov.uk
DEFAULT_UPSTREAM_CMS_MODERN_DOMAIN_NAME=modern-default.cps.gov.uk

