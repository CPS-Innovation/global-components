       *\tEffects\t:\tNone\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\tNone\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tNothing\r\n\t\t\t        */\r\n\r\n\t\t\t        var sMenuURL = \"\";\r\n\t\t\t        var arrWinHand;\r\n\t\t\t        var bFoundWindow = false;\r\n\r\n\t\t\t        InitAutoSearchVars(); // OR4976 - CCR333 - Change GE4\r\n\r\n\t\t\t        // Get a reference to the master window.\r\n\t\t\t        objMasterWindow = top.opener;\r\n\r\n\t\t\t        while (objMasterWindow && objMasterWindow.top.opener) {\r\n\t\t\t            objMasterWindow = objMasterWindow.top.opener;\r\n\t\t\t        }\r\n\r\n\t\t\t        // Used for master window only\r\n\t\t\t        if (!objMasterWindow) {\r\n\t\t\t            objMasterWindow = window;\r\n\t\t\t            bIsMaster = true;\r\n\r\n\t\t\t            // OR8893: Case Locking. Set the window ID\r\n\t\t\t            top.name = \"MASTER\";\r\n\t\t\t            setWindowIDCookie();\r\n\r\n\t\t\t            // Set-up custom data-store\r\n\t\t\t            arrDataStoreName = new Array();\r\n\t\t\t            arrDataStoreValue = new Array();\r\n\r\n\t\t\t            // Redirect frameMain to login\r\n                        if (bInRange && sApplicationMode == CONST_WMS_MODE) {\r\n                             top.frameMain.location.href = \"../../User/uaulLoginCJX.aspx\";\r\n                        }\r\n                        else {\r\n                            top.frameMain.location.href = \"../../User/uaulLogin.aspx\";\r\n                        }\r\n\t\t\t        }\r\n\t\t\t        else {\r\n\t\t\t            objMasterWindow = objMasterWindow.top.frameData;\r\n\r\n\t\t\t            arrWinHand = objMasterWindow.arrWindowHandle;\r\n\r\n\t\t\t            // OR8893: Allocate a name to the child window (this will be 1-5)\r\n\t\t\t            // Determine the highest number that is not currently assigned to a window\r\n\t\t\t            for (var i = 1; i < 6; i++) {\r\n\t\t\t                bFoundWindow = false;\r\n\t\t\t                for (var j = 0; j < arrWinHand.length; j++) {\r\n\t\t\t                    if (arrWinHand[j].top.name == i) {\r\n\t\t\t                        bFoundWindow = true;\r\n\t\t\t                        break;\r\n\t\t\t                    }\r\n\t\t\t                }\r\n\t\t\t                if (bFoundWindow == false) {\r\n\t\t\t                    if (top.name.indexOf(\"CMSPOPUP\") == -1) {\r\n\t\t\t                        top.name = i;\r\n\t\t\t                    }\r\n\t\t\t                    break;\r\n\t\t\t                }\r\n\t\t\t            }\r\n\r\n\t\t\t            // OR8893: Case Locking. Set the window ID\r\n\t\t\t            setWindowIDCookie();\r\n\r\n\t\t\t            // Point datastore to master window\r\n\t\t\t            arrDataStoreName = objMasterWindow.arrDataStoreName;\r\n\t\t\t            arrDataStoreValue = objMasterWindow.arrDataStoreValue;\r\n\r\n\t\t\t            // Navigate to menu URL supplied if there was one...\r\n\t\t\t            sMenuURL = getQSParameter(\"txtMenuURL\", top.document.location.search);\r\n\t\t\t            if (sMenuURL != \"\") {\r\n\t\t\t                // Go back a directory (as in only \"../Dir/Page.aspx\" is provided)\r\n\t\t\t                top.frameMain.location.href = \"../\" + unescape(sMenuURL);\r\n\t\t\t            }\r\n\t\t\t            else {\r\n\t\t\t                top.frameMain.location.href = top.opener.top.frameMain.document.location.href;\r\n\t\t\t            }\r\n\t\t\t        }\r\n\t\t\t    }\r\n\r\n\t\t\t    function InitAutoSearchVars() {\r\n\t\t\t        /*\r\n\t\t\t        *\tPurpose\t:\tOR4976 - CCR333 - Change GE4\r\n\t\t\t        *\t\t\t\tInitialise autosearch values. These will be populated when user searches and persisted here.\r\n\t\t\t        *\t\r\n\t\t\t        *\tEffects\t:\tNone\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\tNone\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tNothing\r\n\t\t\t        */\r\n\r\n\t\t\t        bAutoSearchCases = false;  // should only be used by SearchCases to determine string. Elsewhere QS should be used.\r\n\t\t\t        bAutoSearchArchiveCases = false; //OR14501\r\n\t\t\t        sSearchCriteriaURL = null;\r\n\t\t\t        sArchiveSearchCriteriaURL = null;\r\n\t\t\t        sPersistCourtDropdownHTML = null;\r\n\t\t\t        sPersistProsDropdownHTML = null;\r\n\t\t\t        sPersistCWDropdownHTML = null;\r\n\t\t\t        sPersistWCODropdownHTML = null; //OR8333\r\n\t\t\t        sPersistWATDropdownHTML = null;\r\n\t\t\t        // Remove populated values stored in Data Store, if they exist.\r\n\t\t\t        removeDataStoreValuePersist(\"sLastProsHTML_FC\");\r\n\t\t\t        removeDataStoreValuePersist(\"sLastCWHTML_FC\");\r\n\t\t\t        removeDataStoreValuePersist(\"iLastArea_FC\");\r\n\t\t\t        removeDataStoreValuePersist(\"iLastUnit_FC\");\r\n\t\t\t        removeDataStoreValuePersist(\"iLastWCU_FC\"); //OR8333\r\n\r\n\t\t\t    }\r\n\r\n\t\t\t    function InitCourtListVars() {\r\n\t\t\t        /*\r\n\t\t\t        *\tPurpose\t:\t Initialise court list search values. \r\n\t\t\t        *\t\r\n\t\t\t        *\tEffects\t:\tNone\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\tNone\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tNothing\r\n\t\t\t        */\r\n\t\t\t        //CR2 : reset variables for HearingDate,CourtName,ScreenMode\r\n\t\t\t        strCLHearingDate = \"\";\r\n\t\t\t        strCLCourtId = \"\";\r\n\t\t\t        strCLScreenMode = \"\";\r\n\t\t\t        bCLRedirected = false;\r\n\t\t\t        bCLFromActionBar = false;\r\n                \r\n                }\r\n\r\n\t\t\t    function removeDataStoreValuePersist(sName) {\r\n\t\t\t        /*\r\n\t\t\t        *\tPurpose\t:\tAdded for OR4976 - CCR333 - Change GE4\r\n\t\t\t        *\t\t\t\tRemoves data from the datastore based on a name supplied\r\n\t\t\t        *\t\r\n\t\t\t        *\tEffects\t:\tNothing\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\tNothing\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tNothing\r\n\t\t\t        *\r\n\t\t\t        *\tComments:\tVariant from uainCommonFunctions based on removeDataStoreValue. \r\n\t\t\t        *\t\t\t\tWe can work directly with the DataStore since it is located in this frame.\r\n\t\t\t        */\r\n\r\n\t\t\t        if (arrDataStoreName != null) {\r\n\t\t\t            for (var iIndex = 0; iIndex < arrDataStoreName.length; iIndex++) {\r\n\t\t\t                if (arrDataStoreName[iIndex] == sName) {\r\n\t\t\t                    arrDataStoreName.splice(iIndex, 1);\r\n\t\t\t                    arrDataStoreValue.splice(iIndex, 1);\r\n\t\t\t                    break;\r\n\t\t\t                }\r\n\t\t\t            }\r\n\t\t\t        } // if datastore is null\t\t\t\r\n\t\t\t    } // end function\r\n\r\n\t\t\t    function getApplicationRoot() {\r\n\t\t\t        /*\r\n\t\t\t        *\tPurpose\t:\treturn an URL of format http://{hostname}/{cmsPath}\r\n\t\t\t        *\t\r\n\t\t\t        *\tEffects\t:\tOR4679 - updated to set APPLICATION_URL_ROOT as well\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\tNone\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tString- APPLICATION_URL_ROOT\r\n\t\t\t        */\r\n\t\t\t        //OR4679 -  All CMS files reside under http://{host}/{cmsPath}\r\n\t\t\t        //\r\n\t\t\t        //          1) obtain {host} using document.location.host\r\n\t\t\t        //          2) obtain {cmsPath} using pattern mathing on document.location.path\r\n\t\t\t        //             (non greedy match up to the second slash)\r\n\t\t\t        //\r\n\t\t\t        //          If {cmsPath} is found set and return global variable APPLICATION_URL_ROOT\r\n\t\t\t        //          If {cmsPath} not found the return APPLICATION_URL_ROOT if it \r\n\t\t\t        //          has been initialised from previous call to the function\r\n\t\t\t        //          Otherwise(unlikely given CMS' use cases), guess what {host} and {cmsPath} \r\n\t\t\t        //          will be and return it, but don't see APPLICATION_URL_ROOT\r\n\r\n\r\n\t\t\t        //Pattern to use to match {cmsPath}\r\n\t\t\t        //Changed '?' to '+' to allow multiple words and full stops in the match.\r\n\t\t\t        var objUrlPattern = /\\/([\\w.]+)\\//;\r\n\t\t\t        var objPathFound = document.location.pathname.match(objUrlPattern);\r\n\r\n\t\t\t        //if {cmsPath is found}\r\n\t\t\t        if (objPathFound != null) {\r\n\t\t\t            if (document.location.host != null &&\r\n\t\t\t            document.location.host != \"\") {\r\n\t\t\t                //{host} and {cmsPath} found\r\n\t\t\t                //set an return APPLICATION_URL_ROOT\r\n\t\t\t                APPLICATION_URL_ROOT = 'https://' +\r\n\t\t\t\t                                document.location.host +\r\n\t\t\t\t                                '/' +\r\n\t\t\t\t                                objPathFound[0] +\r\n\t\t\t\t                                '/';\r\n\t\t\t                return APPLICATION_URL_ROOT;\r\n\t\t\t            }\r\n\t\t\t            else {\r\n\t\t\t                //return a guess but don't set APPLICATION_URL_ROOT\r\n\t\t\t                'http://' +\r\n\t\t\t\t        DEFAULT_HOST +\r\n\t\t\t\t        '/' +\r\n\t\t\t\t        objPathFound[0] +\r\n\t\t\t\t        '/';\r\n\t\t\t            }   //end if document.location.host!= null && document.location.host!=\"\"\r\n\t\t\t        }   //end if objPathFound!=null\r\n\r\n\r\n\t\t\t        //{host} and/or {cmsPath} not found- \r\n\t\t\t        //don't set APPLICATION_URL_ROOT \r\n\t\t\t        //but return best guess of application URL root\r\n\t\t\t        if (document.location.host != null &&\r\n\t\t\t        document.location.host != \"\") {\r\n\t\t\t            //{host} found, guess {cmsPath}\r\n\t\t\t            return 'http://' +\r\n\t\t\t\t            document.location.host +\r\n\t\t\t\t            '/' +\r\n\t\t\t\t            DEFAULT_CMS_PATH +\r\n\t\t\t\t            '/';\r\n\r\n\t\t\t        }\r\n\t\t\t        else {\r\n\t\t\t            //{host} and {cmsPath} not found\r\n\t\t\t            //guess both\r\n\t\t\t            return 'http://' +\r\n\t\t\t\t            DEFAULT_HOST +\r\n\t\t\t\t            '/' +\r\n\t\t\t\t            DEFAULT_CMS_PATH +\r\n\t\t\t\t            '/';\r\n\t\t\t        }\r\n\t\t\t    }   //end function getApplicationRoot\r\n\r\n\t\t\t    function setApplicationRoot() {\r\n\t\t\t        /*\r\n\t\t\t        *\tPurpose\t:\tsetting APPLICATION_URL_ROOT using the current document.location\r\n\t\t\t        *               to work out the root.\r\n\t\t\t        *\t\r\n\t\t\t        *\tEffects\t:\tNone\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\t\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tNothing\r\n\t\t\t        */\r\n\t\t\t        //OR4679 invokes getApplicationRoot() which attempts to set APPLICATION_URL_ROOT\r\n\t\t\t        getApplicationRoot();\r\n\r\n\t\t\t    }\r\n\r\n\t\t\t    function getOriginalQueryString() {\r\n\t\t\t        /*\r\n\t\t\t        *\tPurpose\t:\tAccessor method for sOriginalQueryString\r\n\t\t\t        *\t\r\n\t\t\t        *\tEffects\t:\tNone\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\tNone\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tString- sOriginalQueryString\r\n\t\t\t        */\r\n\t\t\t        return sOriginalQueryString;\r\n\t\t\t    }\r\n\r\n\t\t\t    function setOriginalQueryString(sNewQueryString) {\r\n\t\t\t        /*\r\n\t\t\t        *\tPurpose\t:\tMutator method for sOriginalQueryString variable\r\n\t\t\t        *\t\r\n\t\t\t        *\tEffects\t:\tNone\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\tsNewQueryString- String\r\n\t\t\t        *               the query key/value pairs from the form\r\n\t\t\t        *               in the Find Cases screen (uacfSearchCrietria.aspx)\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tNothing\r\n\t\t\t        */\r\n\t\t\t        sOriginalQueryString = sNewQueryString;\r\n\t\t\t    }\r\n\r\n\t\t\t    // Only called on master window\r\n\t\t\t    function closeChildWindows() {\r\n\t\t\t        /*\r\n\t\t\t        *\tPurpose\t:\tCloses all child windows.\r\n\t\t\t        *\t\r\n\t\t\t        *\tEffects\t:\tNone\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\tNone\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tNothing\r\n\t\t\t        */\r\n\r\n\t\t\t        var arrWinHand;\r\n\r\n\t\t\t        arrWinHand = objMasterWindow.arrWindowHandle;\r\n\t\t\t        // Loop thru the child windows (backwards to avoid closing a parent of a child first)\r\n\t\t\t        for (var iMainWinIdx = arrWinHand.length - 1; iMainWinIdx >= 0; iMainWinIdx--) {\r\n\t\t\t            // For each child window, loop thru any popup windows and close\r\n\t\t\t            for (var iPopupIdx = 0; iPopupIdx < arrWinHand[iMainWinIdx].top.frameData.arrPopupWindows.length; iPopupIdx++) {\r\n\t\t\t                // Check if exists - trap a possible error\r\n\t\t\t                try {\r\n\t\t\t                    if (arrWinHand[iMainWinIdx].top.frameData.arrPopupWindows[iPopupIdx][ARRAY_WINDOW_OBJECT]) {\r\n\t\t\t                        arrWinHand[iMainWinIdx].top.frameData.arrPopupWindows[iPopupIdx][ARRAY_WINDOW_OBJECT].timeoutClose();\r\n\t\t\t                    }\r\n\t\t\t                }\r\n\t\t\t                catch (e) { }\r\n\t\t\t            }\r\n\t\t\t            // Close the child\r\n\t\t\t            arrWinHand[iMainWinIdx].close();\r\n\t\t\t        }\r\n\r\n\t\t\t        //OR12507 close the popup if present\r\n\t\t\t        closeOverviewPopup();\r\n\t\t\t    }\r\n\r\n\t\t\t    // Only called on master window\r\n\t\t\t    function closeAllWindows() {\r\n\t\t\t        /*\r\n\t\t\t        *\tPurpose\t:\tCloses all windows (except master).\r\n\t\t\t        *\t\r\n\t\t\t        *\tEffects\t:\tNone\r\n\t\t\t        *\r\n\t\t\t        *\tInputs\t:\tNone\r\n\t\t\t        *\t\r\n\t\t\t        *\tReturns\t:\tNothing\r\n\t\t\t        */\r\n\r\n\t\t\t        closeChildWindows();\r\n\r\n\t\t\t        // Loop thru any popup windows of the master window and close\r\n\t\t\t        f