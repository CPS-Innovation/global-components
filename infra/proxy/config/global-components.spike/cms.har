;\r\nvar strMaterialType;\r\nvar iScreenCaseID;\r\nvar sURN;\r\nvar DOC_ROOT_DIR;\r\nvar indexes;\r\nvar glob_objWordAppPrint = null;\r\nvar glob_iTimeWaitedForWord = 0;\r\nvar glob_defaultprinter = \"\";\r\nvar glob_bPrintWordSuccess = false;\r\nvar glob_blnBackgroundPrintFinished = false;\r\nvar glob_bShowDialog = false;\r\nvar ROW_TYPE_DATA = 0;\r\nvar printCancelled = false;\r\nvar callingWindow;\r\nvar glob_bShowDialogForFirstTime = true;\r\n\nvar glob_currentprinter = \"\";\r\n\nvar glob_bIncludeReviewDoc = false;\r\nvar glob_bShowReviewDialog = false;\r\nvar glob_sReviewQueryString;\r\n\r\nfunction GetLocalGSPointer()\r\n{\r\n\t\r\n\t \r\n\tvar objLocalGSPointer = window.top.frameServerJS;\r\n\tif (typeof objLocalGSPointer == 'undefined' || typeof objLocalGSPointer.APPLICATION_MODE == 'undefined')\r\n\t{\r\n\t\tobjLocalGSPointer = objMainWindow.top.frameData.objMasterWindow.top.frameServerJS;\r\n\t}\r\n\treturn objLocalGSPointer;\r\n}\r\nfunction PrintView(sMenuOption, sFrame, sQueryString)\r\n{\r\n\t\r\n\r\n\tvar sModuleCode = GetScreenContext(sFrame, top);\r\n\tif (sModuleCode == 'uacd')\r\n\t\tsModuleCode = GetScreenContext(FRAME_PAGE, top.frameMain);\r\n\t\t\r\n    if (sQueryString != null)\r\n    {\r\n\t\tswitch (sMenuOption)\r\n\t\t{\r\n\t\t\tcase MENU_PRINT_CURRENT:\r\n\t\t\t\tif(sModuleCode != false){\r\n\t\t\t\t\tPrintCurrent(sModuleCode,sQueryString,sFrame);\r\n\t\t\t\t}\r\n\t\t\t\tbreak;\r\n\t\t\tcase MENU_PRINT_ACTION_PLAN:\r\n\t\t\t\tprintActionPlan(sQueryString);\r\n\t\t\t\tbreak;\r\n\t\t\tcase MENU_PRINT_ENDORSEMENTS:\r\n\t\t\t\tPrintEndorsements(sQueryString);\r\n\t\t\t\tbreak;\r\n\t\t\tcase MENU_PRINT_HEARINGS:\r\n\t     \t\tPrintHearings(sQueryString);\r\n\t\t\t\tbreak;\r\n\t\t\tcase MENU_PRINT_LABELS:\r\n\t\t\t\tPrintLabel(sQueryString);\r\n\t\t\t\treturn;\r\n\t\t\t\tbreak;\r\n\t\t\tcase MENU_PRINT_NOTES:\r\n\t\t\t\tPrintCaseNotes(sQueryString);\r\n\t\t\t\tbreak;\r\n\t\t\tdefault:\r\n\n\n\t\t\t\talert(ERR_PRINT_INVALIDMENU);\r\n\t\t\t\treturn false;\r\n\t\t\t\tbreak;\r\n\t\t}   //end switch\r\n\t}   //end if\r\n\n}\r\n\r\nfunction GetScreenQuery()\r\n{\r\n\t\r\n\r\n\tvar sQueryValue = \"\";\r\n\t\r\n\tsQueryValue = top.frameData.getOriginalQueryString();\r\n\tif(typeof sQueryValue == \"undefined\" || \r\n\t    sQueryValue == \"\" ||\r\n\t        sQueryValue == null)\r\n\t{\r\n\n\n\t\talert(ERR_PRINT_NOFRAMECONTEXT);\r\n\t\treturn null;\r\n\t} \r\n\telse \r\n\t{\r\n\t    return sQueryValue;\t\r\n\t}\r\n\t\r\n}\r\n\r\n\r\nfunction PrintCurrent(sModuleCode,sQueryString,sFrame)\r\n{\r\n\t\r\n\n\n\r\n\tvar bPortrait = true;\r\n\tvar sPrintViewPage;\r\n\tvar sPrintViewPath;\r\n\tvar sTempQueryString;\r\n\tvar objLocalGSPointer;\r\n\t\r\n\tobjLocalGSPointer = GetLocalGSPointer(); // OR11200\r\n\n\tif(objLocalGSPointer.APPLICATION_MODE == \"CMS\"){ \r\n\t\tswitch (sModuleCode) {\r\n\t\t\tcase 'uaco':\t//Case Details - Overview H.2\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uacoPrintView.aspx';\r\n\t\t\t\tbreak;\r\n\t\t\tcase 'uadc':\t//Case Details - Defendants and Charges H.3\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uadcPrintView.aspx';\r\n\t\t\t\tbreak;\r\n\t\t\tcase 'uacw':\t//Case Details - Witnesses H.4\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uacwPrintView.aspx';\r\n\t\t\t\tbreak;\r\n\t\t\tcase 'uacx':\t//Case Details - Exhibits H.5\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uacxPrintView.aspx';\r\n\t\t\t\tbreak;\r\n\t\t\tcase 'uadl':\t//Case Details - Documents H.6\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uadlPrintView.aspx';\r\n\t\t\t\tbreak;\r\n\t\t\tcase 'uacc':\t//Case Details - Contacts H.7\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uaccPrintView.aspx';\r\n\t\t\t\tbreak;\r\n\t\t\tcase 'uacn':\t//Case Details - Communications H.8\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uacnPrintView.aspx';\r\n\t\t\t\tbPortrait = false;\r\n\t\t\t\tbreak;\t\t\t\r\n\t\t\tcase 'uach':\t//Case Details - History H.9\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uachPrintView.aspx';\r\n\t\t\t\tbPortrait = false;\r\n\t\t\t\tbreak;\t\t\t\r\n\t\t\tcase 'uacg':    //Case Details - ??\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uacgPrintView.aspx';\r\n\n\t\t\t\tsTempQueryString = top.frameMain.frames[FRAME_PAGE].location.search.substring(1);\r\n\t\t\t\tsTempQueryString = getQSParameter(QUERY_CALLING_MODE, sTempQueryString);\r\n\n\t\t\t\tsQueryString = sQueryString + '&' + QUERY_CALLING_MODE + '=' + sTempQueryString;\r\n\t\t\t\tif (sTempQueryString== QUERY_WITNESS_MODE)\r\n\t\t\t\t{\r\n\n\t\t\t\t\tsQueryString = sQueryString + '&' + QUERY_WITNESS_SEL_ID + '=' + top.frameMain.framePage.document.frmMain.hidWitnessSelection.value;\r\n\t\t\t\t}\r\n\t\t\t\tbreak;\r\n\t\t\tcase 'uacf':\t//Case Details - Find Case H.15\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Support/';\r\n\t\t\t\tsPrintViewPage = 'uacfPrintView.aspx';\r\n\t\t\t\tsQueryString = \"?\" + GetScreenQuery();\r\n\t\t\t\tbPortrait = false;\r\n\t\t\t\tbreak;\r\n\t\t\tcase 'uavd':    //Advice Case Details H.16\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uavdPrintView.aspx';\r\n\t\t\t\tbreak;\r\n\t\t\tcase 'uact':\t// OR4960 - CTL Details - H.18\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uactPrintView.aspx';\r\n\t\t\t\tbPortrait = false;\r\n\t\t\t\tbreak;\t\t\t\r\n\t\t\tcase 'uatl':    // OR8750 - Task List\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Tasks/';\r\n\t\t\t\tsPrintViewPage = 'uatlPrintView.aspx';\r\n\t\t\t\tbPortrait = false;\r\n\n\t\t\t\tsQueryString = removeParamFromURL(sQueryString, '__VIEWSTATE');\r\n\t\t\t\tbreak;\t\t\t\t\r\n\t\t\tcase 'uaca':\t//OR10733 - Case Allocation - H.23\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\t\t\t\tsPrintViewPage = 'uacaPrintView.aspx';\r\n\n\t\t\t\tsQueryString += \"&\" + QUERY_UNIT_ID + \"=\" + top.frameMain.getUnit();\r\n\t\t\t\tsQueryString += \"&\" + QUERY_UNIT_NAME + \"=\" + top.frameMain.getUnitName();\r\n\t\t\t\tsQueryString += \"&\" + QUERY_RESOURCE_TYPE_ID + \"=\" + top.frameMain.getSelectedResourceType();\t\t\t\t\t\t\t\t\r\n\t\t\t\tsQueryString += \"&\" + QUERY_ROLE_TYPE + \"=\" + top.frameMain.document.frmMain.cboNRole.options(top.frameMain.document.frmMain.cboNRole.selectedIndex).text;\t\t\t\t\t\t\t\t\t\t\t\t\r\n\t\t\t\tbPortrait = false;\r\n\t\t\t\tbreak;\t\t\t\r\n\t\t\tcase 'uapd':\t//OR12521 - Directions for Case/Unit\r\n\t\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'Directions/';\r\n\t\t\t\tsPrintViewPage = 'uapdPrintView.aspx';\r\n\t\t\t\tbPortrait = false;\r\n\t\t\t\tbreak;\r\n\t\t\tdefault:\t\t//Unknown sModuleCode\r\n\n\n\t\t\t\talert(ERR_PRINT_NOPRINTVIEW);\r\n\t\t\t\treturn false;\r\n\t\t\t\tbreak;\r\n\t\t}\r\n\t}\r\n\telse\r\n\t{\r\n\r\n\t\tswitch (sModuleCode) {\r\n\t\tcase 'uawf':    //uawf find cases\r\n\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'WMSSupport/';\r\n\t\t\tsPrintViewPage = 'uawfWMSPrintView.aspx';\t\t\t\r\n\t\t\tsQueryString = \"?\" + GetScreenQuery();\r\n\t\t\tbPortrait = false;\r\n\t\t\tbreak;\r\n\t\tcase 'uade':\t//case details\r\n\t\t\tsPrintViewPath = top.frameData.getApplicationRoot() + 'WMSCase/';\r\n\t\t\tsPrintViewPage = top.frameMain.GetPrintPage();\r\n\t\t\tsQueryString = top.frameMain.GetPrintQueryString();\r\n\n\t\t\t\r\n\t\t\tif( sPrintViewPage == \"uachWMSPrintView.aspx?\" \r\n\t\t\t  ||sPrintViewPage == \"uacgWMSContactLogPrintView.aspx?\"\r\n\t\t\t  ||sPrintViewPage == \"uacnWMSPrintView.aspx?\" ) // OR8906 - Communications Print View should be landscape.\r\n\t\t\t{\r\n\t\t\t\tbPortrait = false;\r\n\t\t\t\t\r\n\t\t\t}\r\n\t\t\telse\r\n\t\t\t{\t\t\t\r\n\t\t\t\tbPortrait = true;\r\n\t\t\t}\r\n\t\t\tif( sQueryString == \"\" ){\r\n\t\t\t\tif( sPrintViewPage == \"uawrWMSCaseDocuments.aspx\" )\r\n\t\t\t\t{\r\n\t\t\t\t\talert(ERR_PRINT_NOPRINTVIEW);\r\n\t\t\t\t}\r\n\t\t\t\telse if (sPrintViewPage == \"\")\r\n\t\t\t\t{\r\n\t\t\t\t\talert(MSG_ERROR_SELECTITEMPRINT);\r\n\t\t\t\t}\r\n\t\t\t\treturn false;\r\n\t\t\t}\r\n\t\t\t\r\n\t\t\tbreak;\t\t\t\r\n\t\tdefault:\t\t//Unknown sModuleCode\r\n\n\n\t\t\talert(ERR_PRINT_NOPRINTVIEW);\r\n\t\t\treturn false;\r\n\t\t\tbreak;\r\n\t\t}\t\r\n\t}\t\r\n\r\n\tprintHidden(sPrintViewPath + sPrintViewPage + sQueryString, bPortrait, sModuleCode);\r\n}\r\n\r\nfunction PrintHearings(sQueryString)\r\n{\r\n\t\r\n\tvar bPortrait = false;\r\n\tvar sPrintViewPath = top.frameData.getApplicationRoot() + 'Hearing/';\r\n\tvar sPrintViewPage = 'uapvHearingHistory.aspx';\r\n\tprintHidden(sPrintViewPath + sPrintViewPage + sQueryString, bPortrait);\r\n}\r\n\r\nfunction PrintCommunications(sQueryString)\r\n{\r\n\t\r\n\tvar bPortrait = true;\r\n\tvar sPrintViewPath = top.frameData.getApplicationRoot() + 'Case/';\r\n\tvar sPrintViewPage = 'uapvCommunications.aspx';\r\n\tprintHidden(sPrintViewPath + sPrintViewPage + sQueryString, bPortrait);\r\n}\r\n\t\r\nfunction PrintEndorsements(sQueryString)\r\n{\r\n\t\r\n\tvar bPortrait = true;\r\n\tvar sPrintViewPath = top.frameData.getApplicationRoot() + \"Case/\";\r\n\tvar sPrintViewPage = \"uapvReviewEndorsements.aspx\";\r\n\n\tvar PRINT_REVIEW_BOTH = \"B\";\r\n\tvar PRINT_REVIEW_STANDARD = \"S\";\r\n\tvar PRINT_REVIEW_DOC = \"D\";\r\n\tvar PRINT_NONE = \"N\";\r\n\tvar QUERY_PRINT_REVIEW = \"strPrintReview\";\r\n\tvar sPrintReview = getQSParameter(QUERY_PRINT_REVIEW, sQueryString);\r\n\n\tif ((sPrintReview == PRINT_REVIEW_BOTH) || (sPrintReview == PRINT_REVIEW_STANDARD) || (sPrintReview == \"\")) {\r\n\t\tprintHidden(sPrintViewPath + sPrintViewPage + sQueryString, bPortrait);\r\n\t}\r\n\n\tif (sPrintReview == PRINT_REVIEW_DOC) {\r\n\n\t\tglob_bShowReviewDialog = (top.frameData.sPrefShoPrntDlog == FLAG_YES) ? true : false;\r\n\t\tprintReviewDoc(sQueryString);\r\n\t}\r\n\n\n\tif (sPrintReview == PRINT_REVIEW_BOTH) {\r\n\t\tglob_bIncludeReviewDoc = true;\r\n\t\tglob_sReviewQueryString = sQueryString;\r\n\t}\r\n}\r\n\r\n\r\nfunction printActionPlan(sQueryString)\r\n{\r\n\t\r\n\tvar bPortrait = true;\r\n\tvar sPrintViewPath;\r\n\tvar sPrintViewPage = \"uapvActionPlan.aspx\";\r\n\r\n\tif (typeof top.frameData == \"undefined\"){\r\n\t\ttop.frameData = window.dialogArguments.top.frameData;\r\n\t}\r\n\t\r\n\tsPrintViewPath = top.frameData.getApplicationRoot() + \"Case/\";\r\n\tprintHidden(sPrintViewPath + sPrintViewPage + sQueryString, bPortrait);\r\n}\r\n\r\n\r\nfunction printReviewDoc(sQueryString)\r\n{\r\n\t\r\n\n\tvar sSaveToPath;\r\n\n\tsSaveToPath = generateReviewDoc(sQueryString);\r\n\t\r\n\tif (sSaveToPath != \"\") {\r\n\t\tif (glob_bIncludeReviewDoc) {\r\n\n\t\t\tPrintCase_PrintDocument(sSaveToPath, factory.printing.copies);\r\n\t\t} else {\r\n\t\t\tPrintCase_PrintDocument(sSaveToPath, 1);\r\n\t\t}\r\n\t}\r\n}\r\nfunction generateReviewDoc(sQueryString)\r\n{\r\n\t\r\n\tvar QUERY_URN = \"txtURN\";\r\n\tvar QUERY_DOC_ID = \"intDocID\";\r\n\tvar QUERY_ORIGINAL_FILENAME = \"strOriginalFilename\";\r\n\tvar QUERY_DMS_PATH = \"strDMSPath\";\r\n\r\n\tsURN = getQSParameter(QUERY_URN, sQueryString);\r\n\tvar iDocId = getQSParameter(QUERY_DOC_ID, sQueryString);\r\n\tvar sDocOriginalFilename = getQSParameter(QUERY_ORIGINAL_FILENAME, sQueryString);\r\n\tvar iCaseId = getQSParameter(QUERY_CASE_ID, sQueryString);\r\n\tvar sDMSPath = getQSParameter(QUERY_DMS_PATH, sQueryString);\r\n\tvar sSaveToPath = \"\";\t// where to save the document when it is got from the DMS\r\n\tvar sDownloadFromPath = \"\";\r\n\tvar sLocalFilename =