\t\t\t iCaseID,\r\n\t\t\t\t\t\t iSessionUserPartyId,\r\n\t\t\t\t\t\t strDispatchResendOptionsCSV,\r\n\t\t\t\t\t\t strEmailOptionsCSV,\r\n\t\t\t\t\t\t iMaterialID,\r\n\t\t\t\t\t\t iVersion,\r\n\t\t\t\t\t\t i32Type,\r\n\t\t\t\t\t\t iEventId,\r\n\t\t\t\t\t\t iEventVersion,\r\n\t\t\t\t\t\t sEventStatus,\r\n\t\t\t\t\t\t i32DocTypeID,\r\n\t\t\t\t\t\t i64DocumentId,\r\n                         arrRecipientEmailAddresses) {\r\n    \r\n\n    var iNewVersionID;\r\n    var i64NewEventId;\r\n\n    var strNewEventStatus;\r\n    var strDispatchDateTime;\r\n\n    displayPleaseWait(true);\r\n\n    strEmailOptionsCSV = strEmailOptionsCSV.replace(/&/g, \"&amp;\");\r\n    if (typeof (arrRecipientEmailAddresses) != \"undefined\") {\r\n        for (var i = 0; i < arrRecipientEmailAddresses.length; i++) {\r\n            arrRecipientEmailAddresses[i] = arrRecipientEmailAddresses[i].replace(/&/g, \"&amp;\");\r\n        }\r\n    }\r\n    intCallID = divService.CMS.callService(DispatchDocPack_Result, \"DispatchDocPack\", iUserID, iCaseID, iSessionUserPartyId, strDispatchResendOptionsCSV, strEmailOptionsCSV, iMaterialID, iVersion, i32Type, iEventId, iEventVersion, sEventStatus, iNewVersionID, i32DocTypeID, i64NewEventId, i64DocumentId, strNewEventStatus, strDispatchDateTime, arrRecipientEmailAddresses);\r\n\r\n}\r\n\r\n\r\nfunction CopyCommsDocument_Result(result) {\r\n    \r\n\r\n    var sPath;                      // holds the function's output parameter.\r\n    var iNewMatId;\r\n    var iNewDocId;\r\n    var sEditorName;\r\n    var iDocPackId;\r\n    var iNewMaterialVersion, iEventID, iEventVersion, iEventRecVersion, iLCommentVersion;\r\n    var bForwardIncoming;\r\n\r\n    var NEW_MATERIAL_ID_PARAMETER = 'i64NewMaterialID'; //the name of the function's output parameter\r\n    var NEW_DOCUMENT_ID_PARAMETER = 'i64NewDocumentID';\r\n    var EDITOR_NAME_PARAMETER = 'strEditorName';\r\n    var DOC_PACK_ID_PARAMETER = 'i64DocPackID';\r\n\n    var NEW_MATERIAL_VERSION_PARAMETER = 'i64NewMaterialVersion';\r\n    var NEW_EVENT_ID_PARAMETER = 'i64EventID';\r\n    var NEW_EVENT_VERSION_PARAMETER = 'i64EventVersion';\r\n    var NEW_EVENT_REC_VERSION_PARAMETER = 'i64EventRecVersion';\r\n    var NEW_LCOMMENT_VERSION_PARAMETER = 'i64LCommentVersion';\r\n    var NEW_FORWARD_INCOMING_PARAMETER = 'blnForwardIncoming';\r\n\r\n    var returnValue;                        // holds the function's return value.\r\n\n    returnValue = result.value['CopyCommsDocumentResult'];\r\n\n\n    iNewMatId = result.value[NEW_MATERIAL_ID_PARAMETER];\r\n    iNewDocId = result.value[NEW_DOCUMENT_ID_PARAMETER];\r\n    sEditorName = result.value[EDITOR_NAME_PARAMETER];\r\n    iDocPackId = result.value[DOC_PACK_ID_PARAMETER];\r\n\n    iNewMaterialVersion = result.value[NEW_MATERIAL_VERSION_PARAMETER];\r\n    iEventID = result.value[NEW_EVENT_ID_PARAMETER];\r\n    iEventVersion = result.value[NEW_EVENT_VERSION_PARAMETER];\r\n    iEventRecVersion = result.value[NEW_EVENT_REC_VERSION_PARAMETER];\r\n    iLCommentVersion = result.value[NEW_LCOMMENT_VERSION_PARAMETER];\r\n    bForwardIncoming = result.value[NEW_FORWARD_INCOMING_PARAMETER];\r\n\n    displayPleaseWait(false);\r\n\n    documentCopied(returnValue, iNewMatId, iNewDocId, sEditorName, iDocPackId, iNewMaterialVersion, iEventID, iEventVersion, iEventRecVersion, iLCommentVersion, bForwardIncoming);\r\n\r\n}\r\n\r\nfunction CopyCommsDocument(iUserID,\r\n\t\t\t\t\t\t   iCaseID,\r\n\t\t\t\t\t\t   iMaterialId,\r\n\t\t\t\t\t\t   iDocId,\r\n\t\t\t\t\t\t   bForwardIncoming,\r\n\t\t\t\t\t\t   bElectronicCopy,\r\n\t\t\t\t\t\t   bLinkReview) {\r\n    \r\n    var iNewMaterialID;\r\n    var iNewDocumentID;\r\n    var sEditorName;\r\n    var iDocPackID;\r\n    var iNewMaterialVersion, iEventID, iEventVersion, iEventRecVersion, iLCommentVersion;\r\n\n    displayPleaseWait(true);\r\n\n    intCallID = divService.CMS.callService(CopyCommsDocument_Result, \"CopyCommsDocument\", iUserID, iCaseID, iMaterialId, iDocId, iNewMaterialID, iNewDocumentID, sEditorName, iDocPackID, iNewMaterialVersion, iEventID, iEventVersion, iEventRecVersion, iLCommentVersion, bForwardIncoming, bElectronicCopy, bLinkReview);\r\n}\r\n\r\nfunction retrieveDocument_Result(result) {\r\n    \r\n    var sPath;                      // holds the function's output parameter.\r\n    var PATH_PARAMETER = 'strPath'; //the name of the function's output parameter\r\n    var sUNCPath; // OR11881 New parameter in the result.\r\n    var UNC_PATH_PARAMETER = 'strUNCPath'; // OR11881\r\n\r\n    var returnValue;                        // holds the function's return value.\r\n\n    var iNewVersionNum;\r\n    var DOC_VERSION_PARAMETER = \"i64Version\";\r\n\r\n    var iDocumentId;\r\n    var DOC_ID_PARAMETER = 'i64OutputDocumentId';\r\n\n    returnValue = result.value['RetrieveDocumentResult'];\r\n\n\n    sPath = result.value[PATH_PARAMETER];\r\n    sUNCPath = result.value[UNC_PATH_PARAMETER]; // OR11881\r\n    iNewVersionNum = result.value[DOC_VERSION_PARAMETER];\r\n    iDocumentId = result.value[DOC_ID_PARAMETER];\r\n\n    displayPleaseWait(false);\r\n\n\n\n\n    documentRetrieved(sPath, returnValue, iNewVersionNum, sUNCPath, iDocumentId); // OR11881 Pass UNC Path to documentRetrieved.\r\n\n}\r\n\r\nfunction retrieveDocument(iUserId, iDocumentId, bSetLastReviewDate) {\r\n    \r\n    var intCallID;\r\n    var sPath;\r\n    var sUNCPath; // OR11881 Retrieve UNC Path of document.\r\n\n    var iVersion;\r\n    var iOutputDocumentId;\r\n\n    displayPleaseWait(true);\r\n\r\n    if (arguments.length == 2) {\r\n        bSetLastReviewDate = false;\r\n    }\r\n\n    if (iDocumentId == 0) {\r\n        alert(\"There is no electronic document for this event\");\r\n    }\r\n    else {\r\n\n        intCallID = divService.CMS.callService(retrieveDocument_Result, \"RetrieveDocument\", iUserId, iScreenCaseID, iDocumentId, bSetLastReviewDate, sPath, sUNCPath, iVersion, iOutputDocumentId); // OR11881\r\n    }\r\n}\r\n\r\nfunction checkInDocument_Result(result) {\r\n    \r\n\r\n    var iNewDocumentId;                      // holds the function's output parameter.\r\n    var iNewDocumentVersion; \t\t\t// OR9003 TP3 - holds the function's output parameter.\r\n    var returnValue;                        // holds the function's return value.\r\n    var DOC_ID_PARAMETER = 'i64NewDocumentid'; //the name of the function's output parameter\r\n    var DOC_VERSION_PARAMETER = 'i64NewDocumentVersion'; //OR9003 TP3\r\n\n    returnValue = result.value['CheckInDocumentResult'];\r\n\n\n    iNewDocumentId = result.value[DOC_ID_PARAMETER];\r\n\n    iNewDocumentVersion = result.value[DOC_VERSION_PARAMETER];\r\n\n    displayPleaseWait(false);\r\n\n    try {\r\n        documentCheckedIn(iNewDocumentId, iNewDocumentVersion, returnValue);\r\n    }\r\n    catch (e) { }\r\n\r\n}\r\n\r\n\r\nfunction checkInDocument(iUserId, iCaseId, iDocumentId, sFilePath, bTemplate, bRepresentationExists, iMaterialId) {\r\n    \r\n    var intCallID;\r\n    var iNewDocumentId;\r\n\n    var iNewDocumentVersion;\r\n\n    sFilePath = encodeHTML(sFilePath, false, true);\r\n\n    displayPleaseWait(true);\r\n\r\n    intCallID = divService.CMS.callService(checkInDocument_Result, \"CheckInDocument\", iUserId, iCaseId, iDocumentId, sFilePath, bTemplate, bRepresentationExists, iMaterialId, iNewDocumentId, iNewDocumentVersion);\r\n\r\n}\r\n\r\nfunction checkInWMSDocument(iUserId, iCaseId, iDocumentId, sFilePath, bTemplate) {\r\n    \r\n    var intCallID;\r\n    var iNewDocumentId;\r\n\n    displayPleaseWait(true);\r\n\r\n    intCallID = divService.CMS.callService(checkInDocument_Result, \"CheckInWMSDocument\", iUserId, iCaseId, iDocumentId, sFilePath, bTemplate, iNewDocumentId);\r\n\r\n}\r\n\r\nfunction checkOutDocument_Result(result) {\r\n    \r\n\r\n    var sPath;                      // holds the function's output parameter.\r\n    var iDocumentId;\r\n    var PATH_PARAMETER = 'strPath';\r\n    var DOC_ID_PARAMETER = 'i64OutputDocumentId';\r\n    var ALT_DOC_ID_PARAMETER = 'i64OutputAltDocumentId';\r\n    var returnValue;                        // holds the function's return value.\r\n    var iAltDocumentId;\r\n    var iDocId;\r\n\n    returnValue = result.value['CheckOutDocumentResult'];\r\n\n\n    sPath = result.value[PATH_PARAMETER];\r\n    iDocumentId = result.value[DOC_ID_PARAMETER];\r\n    iAltDocumentId = result.value[ALT_DOC_ID_PARAMETER];\r\n\n    if ((document.frmMain.hidDocumentActiveID != null) && (document.frmMain.hidDocumentActiveID.value != iDocumentId)) {\r\n        document.frmMain.hidDocumentActiveID.value = iDocumentId;\r\n    }\r\n\n    displayPleaseWait(false);\r\n\r\n    iDocId = iDocumentId;\r\n    if (returnValue !== -1 && iAltDocumentId > 0) {\r\n        iDocId = iAltDocumentId;\r\n    }\r\n\n    try {\r\n        documentCheckedOut(sPath, iDocId, returnValue);\r\n    }\r\n    catch (e) {\r\n\n    }\r\n\r\n}\r\n\nfunction checkOutDocument(iUserId, iCaseId, iDocumentId, iAltDocumentId, iMaterialId, iUnitId, bTemplate) {\r\n    \r\n    var intCallID;\r\n    var sPath;\r\n    var iOutputDocumentId;\r\n    var iOutputAltDocumentId;\r\n\n    displayPleaseWait(true);\r\n\n    intCallID = divService.CMS.callService(checkOutDocument_Result, \"CheckOutDocument\", iUserId, iCaseId, iDocumentId, iAltDocumentId, iMaterialId, iUnitId, bTemplate, sPath, iOutputDocumentId, iOutputAltDocumentId);\r\n}\r\n\r\n\r\nfunction undoCheckOut_Result(result) {\r\n    \r\n\r\n    var returnValue;        // holds the function's return value.\r\n\n    returnValue = result.value;\r\n\n    displayPleaseWait(false);\r\n\n    try {\r\n        checkOutCancelled(returnValue);\r\n    }\r\n    catch (e) { }\r\n\r\n}\r\n\r\n\r\nfunction undoCheckOut(iUserId, iCaseId, iDocumentId, bTemplate) {\r\n    \r\n    var intCallID;\r\n\n    displayPleaseWait(true);\r\n\r\n    intCallID = divService.CMS.callService(undoCheckOut_Result, \"UndoCheckOut\", iUserId, iCaseId, iDocumentId, bTemplate);\r\n\r\n}\r\n\r\n\r\nfunction baselineDocument_Result(result) {\r\n    \r\n\n    var iNewEditedDocumentId;                      // holds the function's output parameter.\r\n    var returnValue;                        // holds the function's return value.\r\n    var DOC_ID_PARAMETER = 'i64NewEditedDocumentId'; //the name of the function's output parameter\r\n\n    returnValue = result.value['BaselineDocumentResult'];\r\n\n\n    iNewEditedDocumentId = result.value[DOC_ID_PARAMETER];\r\n\n    displayPleaseWait(false);\r\n\n    try {\r\n        documentBaselined(iNewEditedDocumentId, returnValue);\r\n    }\r\n    catch (e) { }\r\n\r\n}\r\n\r\n\r\nfunction baselineDocument(iUserId, iCaseId, iDocumentId) {\r\n    \r\n    var intCallID;\r\n\n    var iNewEditedDocumentId;\r\n\n    displayPleaseWait(true);\r\n\r\n    intCallID = divService.CMS.callService(baselineDocument_Result, \"BaselineDocument\", iUserId, iCaseId, iDocumentId, iNewEditedDocumentId);\r\n}\r\n\n\nfunction findAndUploadFileToWebserver(sURN, iCaseId, sFilename, sDocRootDir, objCommonDialog) {\r\n    \r\n\r\n\r\n    var bRetVal;\r\n    var sPath;\r\n\r\n    sPath = getLocalFilePath(sURN, iCaseId, sFilename, sDocRootDir);\r\n\r\n    if (sPath == null) {\r\n        alert('File ' + sFilename + ' not found');\r\n\n\n        sPath = findFile(objCommonDialog, sFilename);\r\n    }\r\n\n    if (sPath != null) {\r\n        bRetVal = uploadFileToWebserver(sPath);\r\n    }\r\n    else {\r\n        return null;\r\n    }\r\n\n    if (bRetVal == true) {\r\n        return sPath;\r\n    }\r\n    else {\r\n        return null;\r\n    }\r\n}\r\n\r\n\r\nfunction uploadFileToWebserver(sSource) {\r\n    \r\n    var bRetVal = false;\r\n\n    sSource = returnPath(sSource);\r\n    bRetVal = uploadDocument(sSource);\r\n    return bRetVal;\r\n}\r\n\r\n\r\nfunction getLocalFilePath(sURN, iCaseId, sFilename, sDocRootDir) {\r\n    \r\n    var objFileSys = new ActiveXObject(\"ucaxLCMG.ucaxWrapper\").GetScripting_FileSystemObject();\r\n    var sReturnedPath = null;\r\n    var sSource;\r\n\n    sSource = getCheckOutPath(sURN, iCaseId, sFilename, sDocRootDir);\r\n\n    if (objFileSys.FileExists(sSource)) {\r\n        sReturnedPath = sSource;\r\n    }\r\n\r\n    objFileSys = null;\r\n\r\n    return sReturnedPath;\r\n}\r\n\r\nfunction getCheckOutPath(sURN, iCaseId, sFilename, sDocRootDir, bMultimedia) {\r\n    \r\n    var NO_URN = 'NOURN';\r\n    var sSource;\r\n    var sWMSSource = new String();\r\n\r\n    sURN = trim(sURN);\r\n\n\n    sURN = replaceIllegalFileNameChars(sURN);\r\n\r\n    if (typeof objGSPointer == 'undefined') {\r\n        if (typeof objMainWindow != 'undefined') {\r\n            objGSPointer = objMainWindow.top.frameData.objMasterWindow.top.frameServerJS;\r\n        }\r\n        else {\r\n            objGSPointer = top.frameData.objMasterWindow.top.frameServerJS;\r\n        }\r\n    }\r\n\r\n    if (typeof objDataPointer == 'undefined') {\r\n        if (typeof objMainWindow != 'undefined') {\r\n            objDataPointer = objMainWindow.top.frameData;\r\n        }\r\n        else {\r\n            objDataPointer = top.frameData;\r\n        }\r\n    }\r\n\r\n    if (objGSPointer.APPLICATION_MODE == \"WMS\" && objGSPointer.SESSION_CJX_LOGGED_ON == true) {\r\n\r\n        sWMSSource = objDataPointer.sPrefDocLocation;\r\n\n        if (sWMSSource == null || sWMSSource == \"\") {\r\n            throw \"sPrefDocLocation has not been set\";\r\n        }\r\n\r\n        if (sWMSSource.lastIndexOf('\\\\') != (sWMSSource.length - 1)) {\r\n            sWMSSource += '\\\\';\r\n        }\r\n        if (bMultimedia) {\r\n            sWMSSource += GetPathLessDrive(objDataPointer.WMS_DOC_ROOT) + '\\\\' + sFilename;\r\n        }\r\n        else {\r\n            sWMSSource += GetPathLessDrive(objDataPointer.WMS_DOC_ROOT) + '\\\\' + sURN + '_' + iCaseId + '\\\\' + sFilename;\r\n        }\r\n        return sWMSSource;\r\n    }\r\n    else {\r\n\r\n        if (sURN.length == 0) {\r\n            sURN = NO_URN;\r\n        }\r\n\r\n        sDocRootDir = returnPath(sDocRootDir); \t\t\t// It will replace any slashes\r\n\n\n        if (sDocRootDir.lastIndexOf('\\\\') != (sDocRootDir.length - 1)) {\r\n            sDocRootDir = sDocRootDir + '\\\\';\r\n        }\r\n\n        if (bMultimedia) {\r\n            sSource = sDocRootDir + '\\\\';\r\n        }\r\n        else {\r\n            sSource = sDocRootDir + sURN + '_' + iCaseId + '\\\\';\r\n        }\r\n\r\n        if (sFilename.indexOf(sSource) == -1) {\r\n            sSource += sFilename;\r\n        }\r\n        else {\r\n            sSource = sFilename;\r\n        }\r\n        return sSource;\r\n    }\r\n}\r\n\n\nfunction findFile(objCommonDialog, sFileName) {\r\n    \r\n    var sReturnedPath = null;\r\n    var sTitle;\r\n    var sFilter;\r\n    var bCloseDialog = false; //OR3863. new boolean indicates whether to end function or not.\r\n\n    if (arguments.length < 2) {\r\n        sFilter = 'Word Documents (*.doc)|*.doc';\r\n        sTitle = 'Find File';\r\n    }\r\n    else {\r\n        sFilter = sFileName + '|' + sFileName;\r\n\n        sTitle = 'Find File: ' + sFileName;\r\n    }\r\n\r\n    objCommonDialog.DialogTitle = sTitle\r\n    objCommonDialog.Filter = sFilter;\r\n\n    while (bCloseDialog == false) {\r\n\n        objCommonDialog.FileName = '';\r\n        objCommonDialog.ShowOpen();\r\n        sReturnedPath = objCommonDialog.FileName;\r\n\n        if (sReturnedPath.length == 0) {\r\n            sReturnedPath = null;\r\n            bCloseDialog = true; //if Cancel is pressed, move on - do not redisplay dialog.\r\n        }\r\n\n\n        else if ((arguments.length == 2) && (filenameOnly(sReturnedPath) != sFileName)) {\r\n            alert('You cannot select this file.\\nPlease select a file named ' + sFileName + '.');\r\n            sReturnedPath = null;\r\n            bCloseDialog = false;\r\n        }\r\n\n        else {\r\n            bCloseDialog = true;\r\n        }\r\n    }\r\n\n\r\n    return sReturnedPath;\r\n}\r\n\r\n\r\nfunction filenameOnly(inString) {\r\n\n    var lastBackSlash;\r\n    var lastForwardSlash;\r\n    var lastSlash;\r\n    var outString;\r\n\r\n    lastBackSlash = inString.lastIndexOf('\\\\', inString.length - 1)\r\n    lastForwardSlash = inString.lastIndexOf('/', inString.length - 1)\r\n\r\n    if (lastBackSlash > lastForwardSlash) {\r\n        lastSlash = lastBackSlash;\r\n    }\r\n    else {\r\n        lastSlash = lastForwardSlash;\r\n    }\r\n\r\n    outString = inString.substring(lastSlash + 1, inString.length)\r\n    return (outString);\r\n}\r\n\r\nfunction returnPath(sPath) {\r\n\n    sPath = sPath.replace(/[\\/]/g, '\\\\')\r\n    return sPath;\r\n}\r\n\nfunction getLocalFilename(iDocId, sOriginalFilename, bEditedDoc) {\r\n    \r\n    var sFilename;\r\n    var sFilenameSuffix;\r\n    var periodIdx;\r\n\r\n    if (bEditedDoc == true) {\r\n        sFilenameSuffix = EDITED_DOC_FILENAME;\r\n    }\r\n    else {\r\n        sFilenameSuffix = VIEWED_DOC_FILENAME;\r\n    }\r\n\r\n    periodIdx = sOriginalFilename.lastIndexOf(\".\");\r\n\r\n    sFilename = iDocId + sOriginalFilename.substring(0, periodIdx) + sFilenameSuffix + sOriginalFilename.substr(periodIdx);\r\n\r\n    return sFilename;\r\n}\r\n\nfunction deleteLocalFile(sURN, iCaseId, sFileName, sDocRootDir) {\r\n    \r\n    var sFilePath;\r\n    var bRetVal = false;\r\n\n    sFilePath = getCheckOutPath(sURN, iCaseId, sFileName, sDocRootDir);\r\n\n    bRetVal = deleteFile(sFilePath);\r\n    return bRetVal;\r\n}\r\n\nfunction deleteFile(sFilePath) {\r\n\n\n    var objFileSys;\r\n    var bRetVal = false;\r\n\n    try {\r\n        objFileSys = new ActiveXObject(\"ucaxLCMG.ucaxWrapper\").GetScripting_FileSystemObject();\r\n    }\r\n    catch (e) {\r\n        DisplayActiveXError();\r\n        return bRetVal;\r\n    }\r\n\n    if (objFileSys.FileExists(sFilePath) == true) {\r\n\n        try {\r\n            objFileSys.DeleteFile(sFilePath, true);\r\n            bRetVal = true;\r\n        }\r\n        catch (e) {\r\n        }\r\n    }\r\n\r\n    return bRetVal;\r\n}\r\n\nfunction copyFile(s