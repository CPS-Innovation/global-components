              <legend class="govuk-fieldset__legend">
                Material classification
              </legend>
              <div class="govuk-checkboxes">
                <div class="govuk-checkboxes__item">
                  <input class="govuk-checkboxes__input" id="classification-1" name="classification" type="checkbox" value="confidential">
                  <label class="govuk-label govuk-checkboxes__label" for="classification-1">
                    Confidential
                  </label>
                </div>
                <div class="govuk-checkboxes__item">
                  <input class="govuk-checkboxes__input" id="classification-2" name="classification" type="checkbox" value="sensitive">
                  <label class="govuk-label govuk-checkboxes__label" for="classification-2">
                    Sensitive
                  </label>
                </div>
                <div class="govuk-checkboxes__item">
                  <input class="govuk-checkboxes__input" id="classification-3" name="classification" type="checkbox" value="restricted">
                  <label class="govuk-label govuk-checkboxes__label" for="classification-3">
                    Restricted access
                  </label>
                </div>
              </div>
            </fieldset>
          </div>
          
          <div class="govuk-form-group">
            <fieldset class="govuk-fieldset">
              <legend class="govuk-fieldset__legend">
                Material handling
              </legend>
              <div class="govuk-radios">
                <div class="govuk-radios__item">
                  <input class="govuk-radios__input" id="handling-1" name="handling" type="radio" value="standard" checked>
                  <label class="govuk-label govuk-radios__label" for="handling-1">
                    Standard handling
                  </label>
                </div>
                <div class="govuk-radios__item">
                  <input class="govuk-radios__input" id="handling-2" name="handling" type="radio" value="special">
                  <label class="govuk-label govuk-radios__label" for="handling-2">
                    Special handling required
                  </label>
                </div>
              </div>
            </fieldset>
          </div>
          
          <div class="govuk-form-group">
            <label class="govuk-label" for="handling-details">
              Special handling details (if required)
            </label>
            <textarea class="govuk-textarea" id="handling-details" name="handling-details" rows="3"></textarea>
          </div>
        </fieldset>
      </div>
      
      <button class="govuk-button" data-module="govuk-button">
        Upload material
      </button>
      
      <!-- Additional actions -->
      <h2 class="govuk-heading-m">Additional actions</h2>
      <div class="govuk-button-group">
        <button class="govuk-button govuk-button--secondary" data-module="govuk-button">
          Download all materials
        </button>
        <button class="govuk-button govuk-button--secondary" data-module="govuk-button">
          Generate materials report
        </button>
        <button class="govuk-button govuk-button--secondary" data-module="govuk-button">
          Share materials
        </button>
      </div>
      
      <!-- Help section -->
      <div class="govuk-inset-text">
        <h2 class="govuk-heading-s">Need help?</h2>
        <p class="govuk-body">If you need assistance managing case materials, please <a href="#" class="govuk-link">view the guidance</a> or <a href="#" class="govuk-link">contact support</a>.</p>
      </div>
    </div>
  </div>
{% endblock %}