{
  "name": "cps-global-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "test.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/cps-global-components/cps-global-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/cps-global-components/cps-global-components.esm.js",
      "require": "./dist/cps-global-components/cps-global-components.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CPS-Innovation/global-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "stencil build",
    "rollup": "rollup --config",
    "predev": "esbuild mock-api/worker.ts --bundle --outfile=www/mock-api/browser.js --format=esm --log-level=error",
    "dev": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "deploy:prod": "dotenvx run -f .env.prod -- bash ./deploy.sh",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@stencil/core": "^4.36.2",
    "@stencil/sass": "^3.0.12",
    "@stencil/store": "^2.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "esbuild": "^0.27.0",
    "jest": "^30.0.0",
    "jest-cli": "^30.0.0",
    "msw": "^2.12.3",
    "navigation-api-types": "^0.6.1",
    "puppeteer": "^24.0.0",
    "rollup": "^4.40.0",
    "rollup-plugin-sourcemaps": "^0.6.3"
  },
  "license": "MIT",
  "dependencies": {
    "@azure/msal-browser": "^5.0.0",
    "@dotenvx/dotenvx": "^1.41.0",
    "@microsoft/applicationinsights-web": "^3.3.6",
    "@types/arrive": "^2.4.4",
    "@types/proper-url-join": "^2.1.5",
    "arrive": "^2.5.2",
    "cps-global-configuration": "workspace:*",
    "cps-global-os-handover": "workspace:*",
    "deepmerge-ts": "^7.1.5",
    "fetch-jsonp": "^1.3.0",
    "govuk-frontend": "^5.7.1",
    "proper-url-join": "^2.1.2",
    "uuid": "^11.1.0",
    "zod": "^4.1.11"
  },
  "msw": {
    "workerDirectory": [
      "www",
      ""
    ]
  }
}
